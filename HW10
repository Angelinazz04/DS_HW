#include <stdio.h>

int main() {
    int tree[] = {1,2,3,4,5};
    int n = 5;

    for(int i = 0; i < n; i++) {
        printf("Parent: %d ", tree[i]);
        if(2*i+1 < n) printf("Left: %d ", tree[2*i+1]);
        if(2*i+2 < n) printf("Right: %d", tree[2*i+2]);
        printf("\n");
    }
}
