#include <stdio.h>

int queue[10], front = 0, rear = -1;

void bfs(int start, int graph[5][5], int visited[]) {
    visited[start] = 1;
    queue[++rear] = start;

    while(front <= rear) {
        int v = queue[front++];
        printf("%d ", v);

        for(int i = 0; i < 5; i++)
            if(graph[v][i] && !visited[i]) {
                visited[i] = 1;
                queue[++rear] = i;
            }
    }
}
